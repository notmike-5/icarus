    %pushi/vec4 45, 0, 256;
    %store/vec4 v0x55e87e08aa60_0, 0, 256;
    %pushi/vec4 9, 0, 256;
    %store/vec4 v0x55e87e08ab30_0, 0, 256;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e87e08aca0_0, 0, 1;
    %delay 51200, 0;
    %delay 1000, 0;
    %vpi_call/w 3 88 "$finish" {0 0 0};
    %end;
    .thread T_7;
